cabal-version: 3.4
name: reading-app
version: 0.1.0.0
license: NONE
build-type: Simple

executable reading-app
    main-is: Main.hs
    hs-source-dirs: exe
    ghc-options: -threaded -Wall
    default-language: GHC2021
    default-extensions:
      RecordWildCards
      LambdaCase
      OverloadedStrings
    build-depends: 
      base ^>=4.19.1.0,
      containers,
      text,
      streaming,
      cc-cedict
      
    other-modules:
